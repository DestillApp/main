{
  "entryPoints": ["src"],
  "entryPointStrategy": "expand",
  "out": "docs/typedoc",
  "tsconfig": "tsconfig.json",
  "includeVersion": true,
  "excludePrivate": true,
  "excludeProtected": true,
  "excludeExternals": true,
  "exclude": [
    "**/*.test.ts",
    "**/*.spec.ts",
    "**/node_modules/**",
    "**/dist/**",
    "src/shims.d.ts",
    "src/shims-vue-icon.d.ts",
    "src/vuex-shim.d.ts"
  ],
  "plugin": ["typedoc-plugin-markdown"]
}
